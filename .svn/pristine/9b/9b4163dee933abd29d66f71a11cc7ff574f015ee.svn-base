<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>Insert title here</title>
</head>
<body>
	<form>
		<table>
			<tr>
				<td>작성자 : </td>
				<td><input type="text" readonly="readonly" value="관리자"></td>
			</tr>
			<tr>
				<td>작성일 : </td>
				<td><input type="date" readonly="readonly" id="now_date"></td>
			</tr>
			<tr>
				<td>제목 : </td>
				<td><input type="text" placeholder="제목을 입력하세요"></td>
			</tr>
			<tr>
				<td>공지사항 : </td>
				<td></td>
			</tr>
		</table>
	</form>
	<script type="text/javascript">
	document.getElementById("now_date").valueAsDate = new Date();
	</script>
</body>
</html>